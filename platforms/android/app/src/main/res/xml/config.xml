<?xml version='1.0' encoding='utf-8'?>
<widget id="TNEducation.MobileApp" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>IGRSMobileApplication</name>
    <description>User can handle the all IGRS releated request or issues through this Application</description>
    <author email="dulasi@trymetech.com" href="https://trymetech.com">
        Trymetech Development Team
    </author>
    <icon density="mdpi" height="57" platform="android" src="src\assets\images\TN_Govt_Logo.png" width="57" />
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="market:*" />
    <preference name="loglevel" value="DEBUG" />
    <preference name="loadUrlTimeoutValue" value="700000" />
</widget>
